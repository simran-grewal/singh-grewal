#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a=6;
    string str;
    int n;
    while(a>0)
    {
        n=a%2;
    str.push_back('0'+n);
    a=a/2;    
    }

    cout<<str;
}
