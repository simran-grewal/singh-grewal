// ---------------------ARRAY OF POINTER---------------------------//
    int *arr[5];
    int arr1[] = {1, 2, 3, 4, 5};
    for(i = 0; i < 5; i++)
    {
        arr[i] = &arr1[i];
       
    }

    for(i = 0; i < 5; i++)
    {
        printf(" %d ", *(*arr)++);
    }

