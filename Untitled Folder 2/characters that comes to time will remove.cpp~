#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    string sr;
    map<char,int> m;
    int i=0;
        cin>>sr;
    while(sr[i]!='\0')
    {
        m[sr[i]]++;        
        i++;
    }
    i=0;
    while(sr[i]!='\0')
    {
        if(m[sr[i]]==1)
        {
            cout<<sr[i];
            //m[sr[i]]++;
        }
        i++;
    }
}
